    "def myfunction(x):\n",
    "    return 2*(x**2)+2\n",
    "\n",
    "def derivative(f, x, e=1e-10):\n",
    "    return (f(x+e) - f(x)) / e\n",
    "\n",
    "x = 1\n",
    "derivative(myfunction,x)"
   ]
  },
