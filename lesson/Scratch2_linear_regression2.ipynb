    "def gradient(f,X,e=1e-10):\n",
    "    grad=[]\n",
    "    for i in range(len(X)):\n",
    "        grad.append(partial_derivative(f,X,i))\n",
    "    return grad\n",
    "\n",
    "X=[1,2,3]\n",
    "gradient(myfunction,X)"
